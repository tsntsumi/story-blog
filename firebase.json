{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": [
    {
      "target": "website",
      "source": "sites/website",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "frameworksBackend": {
        "region": "us-central1"
      }
    },{
      "target": "meet",
      "source": "sites/meet",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "frameworksBackend": {
        "region": "us-central1"
      }
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "hosting": {
      "host": "192.168.0.10",
      "port": 5501
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
